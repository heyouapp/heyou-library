{
    "name": "heyou-library",
    "version": "1.0.21",
    "main": "lib/index.js",
    "module": "lib/index.js",
    "types": "lib/index.d.ts",
    "private": true,
    "repository": {
        "type": "git",
        "url": "https://github.com/heyouapp/heyou-library.git"
    },
    "scripts": {
        "prebuild": "rm -rf lib",
        "build": "yarn build:types && yarn build:js",
        "build:types": "ttsc --emitDeclarationOnly",
        "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" -D --source-maps inline",
        "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
        "sb": "start-storybook -p 9009 -s public",
        "sb:build": "build-storybook -s public",
        "sb:deploy": "storybook-to-ghpages -e storybook-static -o storybook-static"
    },
    "peerDependencies": {
        "react": "*",
        "react-native": "*"
    },
    "dependencies": {
        "expo-blur": "~8.0.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.7.7",
        "@babel/core": "^7.8.3",
        "@babel/runtime": "^7.8.3",
        "@expo/webpack-config": "^0.10.11",
        "@react-native-community/eslint-config": "^0.0.5",
        "@storybook/addon-actions": "^5.3.8",
        "@storybook/addon-info": "^5.3.8",
        "@storybook/addon-links": "^5.3.8",
        "@storybook/addons": "^5.3.8",
        "@storybook/preset-typescript": "^1.2.0",
        "@storybook/react": "^5.3.8",
        "@storybook/storybook-deployer": "^2.8.1",
        "@types/react": "^16.9.0",
        "@types/react-dom": "^16.9.0",
        "@types/react-native": "^0.60.30",
        "@typescript-eslint/eslint-plugin": "^2.12.0",
        "@typescript-eslint/parser": "^2.12.0",
        "babel-plugin-module-resolver": "^4.0.0",
        "babel-preset-expo": "^8.0.0",
        "eslint": "^6.5.1",
        "fork-ts-checker-webpack-plugin": "^4.0.2",
        "react-docgen-typescript-loader": "^3.6.0",
        "react-dom": "^16.12.0",
        "react-native": "^0.61.5",
        "react-native-web": "^0.12.0-rc.1",
        "ts-loader": "^6.2.1",
        "ttypescript": "^1.5.10",
        "typescript": "^3.7.3",
        "typescript-transform-paths": "^1.1.14"
    },
    "files": [
        "lib"
    ]
